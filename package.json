{
  "version": "0.0.0-this-version-will-be-set-from-ci",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsup src/index.ts --watch",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "build": "yarn test && tsup src/index.ts --minify --dts --format esm,cjs --legacy-output",
    "prepare": "yarn build",
    "lint": "yarn prettier --check \"{src,test}/**/*.{ts,tsx,js,jsx,md}\"",
    "format": "yarn prettier --write \"{src,test}/**/*.{ts,tsx,js,jsx,md}\""
  },
  "peerDependencies": {
    "effector": "^22",
    "history": "^5.3.0"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.0.1",
    "@types/history": "^5.0.0",
    "@vitest/ui": "~0.22.1",
    "c8": "^7.12.0",
    "effector": "^22",
    "history": "^5.3.0",
    "jsdom": "~20.0.0",
    "prettier": "^2.7.1",
    "size-limit": "^8.0.1",
    "tslib": "^2.4.0",
    "tsup": "^6.2.2",
    "typescript": "^4.6.3",
    "vitest": "~0.22.1"
  },
  "dependencies": {
    "path-to-regexp": "^6.2.0"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "atomic-router",
  "author": "Anton Kosykh",
  "repository": {
    "type": "git",
    "url": "https://github.com/kelin2025/atomic-router"
  },
  "size-limit": [
    {
      "path": "dist/esm/index.js",
      "limit": "10 kB"
    },
    {
      "path": "dist/index.js",
      "limit": "10 kB"
    }
  ]
}
